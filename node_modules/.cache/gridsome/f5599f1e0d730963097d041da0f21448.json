\"flex justify-end\">\n            <!-- <p class=\"opacity-25\">\n              {{ $page.strapi.articles[0].published_at | dateFormat }}\n            </p> -->\n\n            <div class=\"flex justify-end items-center\">\n              <!-- <span class=\"uppercase pr-4 pt-1 opacity-25\">Поделиться</span> -->\n              <YandexShare />\n            </div>\n          </div>\n          <!-- <hr /> -->\n\n          <!-- <div class=\"flex items-center pt-8\">\n            <div class=\"w-1/6\">\n              <g-image\n                v-if=\"$page.strapi.articles[0].user.image.url\"\n                class=\"rounded-full\"\n                width=\"100\"\n                :src=\"$page.strapi.articles[0].user.image.url\"\n                alt=\"\"\n              />\n            </div>\n            <div class=\"w-5/6 pl-8\">\n              <p class=\"font-bold text-xl pb-0\">\n                {{ $page.strapi.articles[0].user.username }}\n              </p>\n              <p class=\"pb-4 text-sm\">независимый партнер HerbaLife</p>\n              <SocIcons class=\"flex items-center\" />\n            </div>\n          </div> -->\n        </div>\n      </div>\n    </div>\n  </Layout>\n</template>\n\n<page-query>\n  query ($slug: String!) {\n    strapi {\n      articles(where: { slug: $slug }) {\n        slug\n        title\n        content\n        publishedAt\n        image {\n          url\n        }\n        Seo {\n          metaTitle\n          metaDescription\n          shareImage {\n            image {\n              url\n            }\n          }\n        }\n        user {\n          username\n          image {\n            url\n          }\n        }\n      }\n\t  }\n  }\n</page-query>\n\n<script>\nimport SocIcons from \"~/components/atoms/SocIcons\"\nimport YandexShare from \"~/components/atoms/YandexShare\"\nimport VueMarkdown from \"vue-markdown\"\n\nvar moment = require(\"moment\")\n\nexport default {\n  data() {\n    return {\n      moment: moment,\n      jsonld: {\n        \"@context\": \"https://schema.org\",\n        \"@type\": \"BreadcrumbList\",\n        itemListElement: [\n          {\n            \"@type\": \"ListItem\",\n            position: 1,\n            item: {\n              \"@id\": process.env.APP_URL,\n              name: \"Главная\",\n            },\n          },\n          {\n            \"@type\": \"ListItem\",\n            position: 2,\n            item: {\n              \"@id\": process.env.APP_URL + \"/blog/\",\n              name: \"Блог\",\n            },\n          },\n          {\n            \"@type\": \"ListItem\",\n            position: 3,\n            item: {\n              \"@id\":\n                process.env.APP_URL +\n                \"/\" +\n                this.$page.strapi.articles[0].slug +\n                \"/\",\n              name: this.$page.strapi.articles[0].title,\n            },\n          },\n        ],\n      },\n    }\n  },\n  filters: {\n    dateFormat: function(date) {\n      return moment(date).format(\"DD MMMM YYYY г.\")\n    },\n  },\n  components: {\n    VueMarkdown,\n    SocIcons,\n    YandexShare,\n  },\n  metaInfo() {\n    return {\n      title: this.$page.strapi.articles[0].Seo.metaTitle,\n      script: [\n        { innerHTML: JSON.stringify(this.jsonld), type: \"application/ld+json\" },\n      ],\n      meta: [\n        {\n          name: \"description\",\n          content: this.$page.strapi.articles[0].Seo.metaDescription,\n        },\n        {\n          property: \"og:title\",\n          content: this.$page.strapi.articles[0].Seo.metaTitle,\n        },\n        {\n          property: \"og:description\",\n          content: this.$page.strapi.articles[0].Seo.metaDescription,\n        },\n        {\n          property: \"og:image\",\n          content: this.$page.strapi.articles[0].Seo.shareImage.image.url,\n        },\n      ],\n    }\n  },\n}\n</script>\n<style lang=\"postcss\" scoped>\n.content p {\n  @apply pb-4;\n}\n.content blockquote {\n  @apply bg-green-100 rounded-md pb-1 px-4 pt-4 my-6 border-green-200;\n  border-left: 1rem #c4e4ac solid;\n}\n</style>\n"],"sourceRoot":"src/templates"}]}