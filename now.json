{
  "name": "zvekov.com",
  "scope": "zvekov",
  "github": {
    "enabled": true
  },
  "trailingSlash": true,
  "rewrites": [
    { "source": "/(.*)", "destination": "404" }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    }
  ]
}
